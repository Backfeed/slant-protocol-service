{
  "functions": {
    "getbidding": {
      "custom": {
        "excludePatterns": [],
        "envVars": []
      },
      "handler": "modules/biddings/getbidding/handler.handler",
      "timeout": 6,
      "memorySize": 1024,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "biddings/{id}",
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": true,
          "requestParameters": {
            "integration.request.querystring.id": "method.request.querystring.id"
          },
          "requestTemplates": {
            "application/json": "{\"id\":\"$input.params('id')\"}"
          },
          "responses": {
            "404": {
              "statusCode": "404",
              "selectionPattern": "404:.*",
              "responseParameters": {},
              "responseTemplates": {
                "application/json": "#set($inputRoot = $input.path('$'))\n{\"error\": \"$inputRoot.errorMessage.substring(4)\"}"
              }
            },
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": ""
              }
            }
          }
        }
      ]
    }
  }
}
